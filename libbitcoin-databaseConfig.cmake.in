# - Config file for the LIBBITCOIN_DATABASE package
#
#  LIBBITCOIN_DATABASE_INCLUDE_DIRS - include directories for LIBBITCOIN_DATABASE
#  LIBBITCOIN_DATABASE_LIBRARIES    - libraries to link against

get_filename_component(LIBBITCOIN_DATABASE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBBITCOIN_DATABASE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

include("${LIBBITCOIN_DATABASE_CMAKE_DIR}/libbitcoin-databaseTargets.cmake")
set(LIBBITCOIN_DATABASE_LIBRARIES libbitcoin-database)

find_package(libbitcoin 3.0.0 REQUIRED)
